<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="639.0" prefWidth="910.0" spacing="10.0" stylesheets="@../../Css/Style.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="Creation Ticket">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
        <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="Titre">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
        <TextField fx:id="Title" />
        <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="Description" />
        <TextField fx:id="description" prefHeight="194.0" prefWidth="870.0" />
        <Label fx:id="lblErreur" alignment="CENTER" maxWidth="1.7976931348623157E308" textFill="RED" />
        <Button mnemonicParsing="false" onAction="#handleAddTicket" text="Sauvegarder" />
        <Button mnemonicParsing="false" text="Retour" />
    </children>
    <padding>
        <Insets left="20.0" right="20.0" />
    </padding>
</VBox>
